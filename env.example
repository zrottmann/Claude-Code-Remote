# Appwrite Configuration
# Copy this file to .env and fill in your values

# Required: Your Appwrite API key with functions.write or sites.write permissions
APPWRITE_API_KEY=your-api-key-here

# Optional: Appwrite project ID (defaults to 68a4e3da0022f3e129d0)
APPWRITE_PROJECT_ID=68a4e3da0022f3e129d0

# Optional: Appwrite endpoint (defaults to https://nyc.cloud.appwrite.io/v1)
APPWRITE_ENDPOINT=https://nyc.cloud.appwrite.io/v1

# Optional: Deployment target (remote, super-site, etc.)
APPWRITE_DEPLOYMENT_TARGET=remote